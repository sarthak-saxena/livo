{
  "compilerOptions": {
	"target": "es5",
	"jsx": "react",
	"allowJs": true,
	"module": "commonjs",
	"moduleResolution": "node",
	"emitDecoratorMetadata": true,
	"experimentalDecorators": true,
	"declaration": true,
	"noImplicitAny": false,
	"noImplicitReturns": false,
	"removeComments": true,
	"strictNullChecks": false,
	"keyofStringsOnly": true,
	"outDir": "./lib",
	"lib": ["es6", "es7", "dom"],
	"types": ["node"],
	"skipLibCheck": true,
	"esModuleInterop": true
  },
  "include": ["src/**/*"],
  "exclude": ["**/node_modules", "**/build", "**/dist", "**/*.md"]
}
